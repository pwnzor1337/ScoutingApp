<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/brick_wall" />
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="closeKeyboard"
            android:layout_below="@+id/textView"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/teamNumberSticker"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="27dp"
                    android:background="@drawable/white_background"
                    android:padding="5dp"
                    android:onClick="closeKeyboard">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="30dp"
                        android:text="Team Number:"
                        android:id="@+id/textView2"
                        android:onClick="closeKeyboard"/>

                    <TextView
                        android:id="@+id/teamNumberDisplay"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textAlignment="center"
                        android:layout_toRightOf="@+id/textView2"
                        android:onClick="closeKeyboard"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rowSticker"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/teamNumberSticker"
                    android:layout_marginTop="27dp"
                    android:background="@drawable/white_background"
                    android:padding="5dp"
                    android:onClick="closeKeyboard">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="30dp"
                        android:text="Average Rows Completed:"
                        android:id="@+id/textView3"
                        android:onClick="closeKeyboard"/>

                    <TextView
                        android:id="@+id/rowDisplay"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textAlignment="center"
                        android:layout_toRightOf="@+id/textView3"
                        android:onClick="closeKeyboard"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/colSticker"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/rowSticker"
                    android:layout_marginTop="27dp"
                    android:background="@drawable/white_background"
                    android:padding="5dp"
                    android:onClick="closeKeyboard">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="30dp"
                        android:text="Average Columns Completed:"
                        android:id="@+id/textView4"
                        android:onClick="closeKeyboard"/>

                    <TextView
                        android:id="@+id/colDisplay"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textAlignment="center"
                        android:layout_toRightOf="@+id/textView4"
                        android:onClick="closeKeyboard"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/cubeSticker"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/colSticker"
                    android:layout_marginTop="27dp"
                    android:background="@drawable/white_background"
                    android:padding="5dp"
                    android:onClick="closeKeyboard">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="30dp"
                        android:text="Average Glyphs (Cubes) Placed:"
                        android:id="@+id/textView5"
                        android:onClick="closeKeyboard"/>

                    <TextView
                        android:id="@+id/cubeDisplay"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textAlignment="center"
                        android:layout_toRightOf="@+id/textView5"
                        android:onClick="closeKeyboard"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/platSticker"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/cubeSticker"
                    android:layout_marginTop="27dp"
                    android:background="@drawable/white_background"
                    android:padding="5dp"
                    android:onClick="closeKeyboard">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="30dp"
                        android:text="Average Returns to Platform:"
                        android:id="@+id/textView6"
                        android:onClick="closeKeyboard"/>

                    <TextView
                        android:id="@+id/platDisplay"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textAlignment="center"
                        android:layout_toRightOf="@+id/textView6"
                        android:onClick="closeKeyboard"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/patternSticker"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/platSticker"
                    android:layout_marginTop="27dp"
                    android:background="@drawable/white_background"
                    android:padding="5dp"
                    android:onClick="closeKeyboard">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="30dp"
                        android:text="Average Cyphers (Patterns):"
                        android:id="@+id/textView7"
                        android:onClick="closeKeyboard"/>

                    <TextView
                        android:id="@+id/patternDisplay"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textAlignment="center"
                        android:layout_toRightOf="@+id/textView7"
                        android:onClick="closeKeyboard"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/jewelSticker"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/patternSticker"
                    android:layout_marginTop="27dp"
                    android:background="@drawable/white_background"
                    android:padding="5dp"
                    android:onClick="closeKeyboard">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="30dp"
                        android:text="Average Jewels:"
                        android:id="@+id/textView8"
                        android:onClick="closeKeyboard"/>

                    <TextView
                        android:id="@+id/jewelDisplay"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textAlignment="center"
                        android:layout_toRightOf="@+id/textView8"
                        android:onClick="closeKeyboard"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/safezoneSticker"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/jewelSticker"
                    android:layout_marginTop="27dp"
                    android:background="@drawable/white_background"
                    android:padding="5dp"
                    android:onClick="closeKeyboard">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="30dp"
                        android:text="Average Safezones:"
                        android:id="@+id/textView9"
                        android:onClick="closeKeyboard"/>

                    <TextView
                        android:id="@+id/safezoneDisplay"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textAlignment="center"
                        android:layout_toRightOf="@+id/textView9"
                        android:onClick="closeKeyboard"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/autoCubeSticker"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/safezoneSticker"
                    android:layout_marginTop="27dp"
                    android:background="@drawable/white_background"
                    android:padding="5dp"
                    android:onClick="closeKeyboard">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="30dp"
                        android:text="Average Glyphs (Cubes) Autonomous:"
                        android:id="@+id/textView10"
                        android:onClick="closeKeyboard"/>

                    <TextView
                        android:id="@+id/autoCubeDisplay"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textAlignment="center"
                        android:layout_toRightOf="@+id/textView10"
                        android:onClick="closeKeyboard"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/correctColSticker"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/autoCubeSticker"
                    android:layout_marginTop="27dp"
                    android:background="@drawable/white_background"
                    android:padding="5dp"
                    android:onClick="closeKeyboard">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="30dp"
                        android:text="Average Correct Column Placements:"
                        android:id="@+id/textView11"
                        android:onClick="closeKeyboard"/>

                    <TextView
                        android:id="@+id/correctColDisplay"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textAlignment="center"
                        android:layout_toRightOf="@+id/textView11"
                        android:onClick="closeKeyboard"/>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/pointsSticker"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/correctColSticker"
                    android:layout_marginTop="27dp"
                    android:background="@drawable/white_background"
                    android:padding="5dp"
                    android:onClick="closeKeyboard">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="30dp"
                        android:text="Average Points:"
                        android:id="@+id/textView12"
                        android:onClick="closeKeyboard"/>

                    <TextView
                        android:id="@+id/pointsDisplay"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:textAlignment="center"
                        android:layout_toRightOf="@+id/textView12"
                        android:onClick="closeKeyboard"/>
                </RelativeLayout>


            </RelativeLayout>
        </ScrollView>

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TEAM INFO"
            android:textAlignment="center"
            android:textColor="#77ff77"
            android:textSize="40sp" />

    </RelativeLayout>

</LinearLayout>